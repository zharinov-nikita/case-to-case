  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@jest/globals": "^29.4.2",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "babel-jest": "^29.4.2",
    "eslint": "^8.34.0",
    "husky": "^8.0.3",
    "jest": "^29.4.2",
    "prettier": "^2.8.4",
    "prettier-plugin-organize-imports": "^3.2.2",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
